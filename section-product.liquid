{%- liquid
  assign full_width = section.settings.full_width
  assign product = all_products[section.settings.product]
  assign screen = ''
  if section.settings.background_img_d != blank or section.settings.background_img_m != blank
    assign background_img = true
  else
    assign background_img = false
  endif         
  if background_img
    capture alt_d
      if section.settings.background_img_d.alt != blank
        echo section.settings.background_img_d.alt
      elsif section.settings.title != blank
        echo section.settings.title
      elsif section.settings.sub_title != blank
        echo section.settings.sub_title
      elsif section.settings.body != blank
        echo section.settings.body
      else
        echo section.settings.background_img_d | split: '/' | last | replace: '-', ' ' | replace: '_', ' ' | split: '.' | first | capitalize
      endif
    endcapture 
    capture alt_m
      if section.settings.background_img_m.alt != blank
        echo section.settings.background_img_m.alt
      elsif section.settings.title != blank
        echo section.settings.title
      elsif section.settings.sub_title != blank
        echo section.settings.sub_title
      elsif section.settings.body != blank
        echo section.settings.body
      else
        echo section.settings.background_img_m | split: '/' | last | replace: '-', ' ' | replace: '_', ' ' | split: '.' | first | capitalize
      endif
    endcapture 
  endif
  if product == empty
    assign section_onboarding = true
    assign product_id = section.id
    assign product_media_size = 1
  else
    assign section_onboarding = false
    assign product_id = section.id
    assign product_media_size = product.media.size
  endif
  assign rtl = false
  assign rtl_list = 'ae,ar,arc,bcc,bqi,ckb,dv,fa,glk,ha,he,kwh,ks,ku,mzn,nqo,pnb,ps,sd,ug,ur,yi'
  assign iso_code = localization.language.iso_code | split: '-' | first
  if rtl_list contains iso_code
    assign rtl = true
  endif
  assign product_variants = false
  assign background_color = false
  if section.settings.background != 'rgba(0,0,0,0)' and section.settings.background != blank
    assign background_color = true
    assign background_color_section = section.settings.background
  endif    
  if section.settings.background_gradient != 'rgba(0,0,0,0)' and section.settings.background_gradient != blank
    assign background_color = true
    assign background_color_section = section.settings.background_gradient
  endif
  capture classes
    echo 'dynamic-section dynamic-section-'
    echo section.id 
    echo ' section-padding-top section-padding-bottom section-margin-bottom '
    if section.settings.padding_border
      echo 'section-border '
    endif
  endcapture
  assign overlay_color = false
  if section.settings.background_img_overlay != 'rgba(0,0,0,0)' and section.settings.background_img_overlay != blank
    assign overlay_color = true
    assign overlay_color_section = section.settings.background_img_overlay
  endif
  if section.settings.background_img_overlay_gradient != 'rgba(0,0,0,0)' and section.settings.background_img_overlay_gradient != blank
    assign overlay_color = true
    assign overlay_color_section = section.settings.background_img_overlay_gradient
  endif
  if section.settings.enable_lazy
    assign lazy = 'lazy'
  else
    assign lazy = 'eager'
  endif
  assign banner = false
-%}

{% style %}
  .dynamic-section-{{ section.id }} { 
    --margin_padding_desktop: {{ section.settings.padding_desktop }}vh;
    --margin_padding_mobile: {{ section.settings.padding_mobile }}vh;
    {%- if background_color -%}
    --background:{{ background_color_section }};
    --button_primary_text:{{ section.settings.background }};
    --button_secondary_hover:{{ section.settings.background }};
    {%- endif -%}
    {%- if overlay_color -%}
    --overlay_background: {{ overlay_color_section }};
    --overlay_opacity: {{ section.settings.background_img_overlay_opacity | times: 0.01 }};
    {%- endif -%}
    --section-padding-top-mobile: {{ section.settings.padding_top_mobile }}px;
    --section-padding-top-desktop: {{ section.settings.padding_top_desktop }}px;
    --section-padding-bottom-mobile: {{ section.settings.padding_bottom_mobile }}px;
    --section-padding-bottom-desktop: {{ section.settings.padding_bottom_desktop }}px;
    --section-margin-bottom-mobile: {{ section.settings.margin_bottom_mobile }}px;
    --section-margin-bottom-desktop: {{ section.settings.margin_bottom_desktop }}px;
  }
{% endstyle %}

<section id="product-id-{{ product_id }}" 
         data-product-id="{{ product_id }}" 
         data-section-id="{{ section.id }}" 
         {% unless section_onboarding %}
         data-section-type="product-alt"
         data-product-type="section" 
         data-url="{{ 'script.product.js' | asset_url }}"
         {% endunless %}
         data-atc="{% if product.tags contains settings.preorder %}{{ settings.preorder }}{% else %}{{ 'products.product.add_to_cart' | t }}{% endif %}"
         data-carousel-type="section"
         data-cart="{{ section.settings.go_to_cart_enable }}"
         data-zoom="{% unless rtl %}{{ section.settings.zoom_enable }}{% else %}false{% endunless %}"
         data-mobile-zoom="{{ section.settings.zoom_mobile_enable }}"
         data-image="{{ product.featured_image | image_url: width: 260 }}"
         data-history="false"
         class="home-product product-id product-{{ product_id }} {{ classes }}{% if settings.animation_enable %}animate-section-div {% endif %}clearfix">

  {% if background_img %}
    <div class="banner-section {% if full_width %}bottompad-half {% if background_img or overlay_color %}relative {% if background_img %}lazycontainer {% endif %}{% if overlay_color %}background-overlay {% endif %}{% endif %}{% endif %}clearfix">
      {%- liquid
        if full_width and section.settings.background_img_d != blank
          assign banner = true
          if section.settings.background_img_m != blank
            assign screen = 'desktop-image '
          endif
        render 'image.load', img_src: section.settings.background_img_d, size: section.settings.background_img_d.width, alt: alt_d, background: true, screen: screen, lazy: lazy, sizes: '100vw'
        endif
        if full_width and section.settings.background_img_m != blank
          assign banner = true
          if section.settings.background_img_d != blank
            assign screen = 'mobile-image '
          endif
        render 'image.load', img_src: section.settings.background_img_m, size: section.settings.background_img_m.width, alt: alt_m, background: true, screen: screen, lazy: lazy, sizes: '100vw'
        endif
      -%}
      <div class="relative container{% if full_width %} half-pad{% else %} {% if background_img or overlay_color %}inner-container{% else %}half-pad{% endif %}{% endif %}{% if settings.animation_enable %} animate-section load{% endif %} clearfix">
        <div class="{% if full_width == false %}{% if background_img or overlay_color %}inner-container-pad relative {% if background_img %}lazycontainer {% endif %}{% if overlay_color %}background-overlay {% endif %}{% endif %}{% endif %}clearfix">
          {%- liquid
            if full_width == false and section.settings.background_img_d != blank
              assign banner = true
              if section.settings.background_img_m != blank
                assign screen = 'desktop-image '
              endif
              render 'image.load', img_src: section.settings.background_img_d, size: section.settings.background_img_d.width, alt: alt_d, background: true, screen: screen, lazy: lazy, sizes: '(min-width: 1220px) 1180px, calc(100vw - 40px)'
            endif
            if full_width == false and section.settings.background_img_m != blank
              assign banner = true
              if section.settings.background_img_d != blank
                assign screen = 'mobile-image '
              endif
              render 'image.load', img_src: section.settings.background_img_m, size: section.settings.background_img_m.width, alt: alt_m, background: true, screen: screen, lazy: lazy, sizes: '(min-width: 1220px) 1180px, calc(100vw - 40px)'
            endif
          -%}
          <div class="relative table content-height content-height-options"></div>
        </div>
      </div>
    </div>
  {% endif %}

  <div class="container {% if banner %}inner-container {% else %}half-pad {% endif %}{% unless section.settings.two_col_enable and product_media_size > 0 %}container-six {% endunless %}{% if settings.animation_enable %}animate-section animate-section-margin load {% endif %}clearfix">
    <div class="{% if banner %}{% unless full_width %}inner-container-grid {% endunless %}{% endif %}tab-body">
      <div class="{% if banner %}background-white {% endif %}table-center clearfix animate">
        <div class="featured-image {% if section.settings.two_col_enable and product_media_size > 0 %}six{% else %}twelve{% endif %} {{ section.settings.vertical_content_alignment }}">
          {%- liquid
            unless section_onboarding
              if product.media.size > 0
                render 'product.images', product: product, rtl: rtl, section_id: section.id, lazy: lazy, sizes_main: '580,360,,60', sizes_thumb: '130,125'
              endif
            else
              echo 'product-1' | placeholder_svg_tag: 'placeholder-svg'
            endunless
          -%}
        </div>
        <div id="product-details-{{ section.id }}" class="product-details {% if section.settings.two_col_enable and product_media_size > 0 %}six product-details-padding{% else %}twelve{% endif %} {{ section.settings.vertical_content_alignment }} {% if product.media.size > 1 %} toppad-quarter{% endif %}{% unless product.media.size > 0 %} no-img{% endunless %} {% if section.settings.centered %}text-center{% else %}text-left{% endif %} clearfix">
          <div class="tab-body{% if section.settings.centered %} center clearfix{% endif %}">          
            {% unless section_onboarding %}

              {%- liquid 
                capture form_id
                  echo 'add-item-form-'
                  echo section.id
                endcapture
                capture form_class
                  if section.settings.go_to_cart_enable
                    echo 'go-to-cart-'
                    echo section.id
                    echo ' '
                  endif
                  echo 'product-form-'
                  echo section.id
                  echo ' variants clearfix'
                endcapture
                assign variant = product.selected_or_first_available_variant
              -%}
  
              {% form 'product', product, id:form_id, class:form_class, data-product-form: '' %}
  
              {%- liquid 
                for block in section.blocks                  
                  case block.type
                    
                    when 'title'
                      if block.settings.title_h1
                        assign title = 1
                      else
                        assign title = 2
                      endif
                      echo '<div class="product-title '
                      if block.settings.divider
                        echo 'form-blocks '
                      endif
                      echo 'clearfix"'
                      echo block.shopify_attributes
                      echo '><h'
                      echo title
                      echo '><a href="'
                      echo product.url
                      echo '" class="section-title-'
                      echo block.settings.title_size
                      echo ' " title="'
                      echo product.title | escape
                      echo '">'
                      echo product.title
                      echo '</a></h'
                      echo title
                      echo '></div>'
                    
                    when 'skuvendor'
                      render 'product.SkuVendor', product: product, block: block, vendor_enable: block.settings.vendor_enable, sku_enable: block.settings.sku_enable, divider: block.settings.divider
                    
                    when 'rating'
                      render 'product.rating', product: product, block: block, product_page: true, divider: block.settings.divider
  
                    when 'price'
                      echo '<div class="'
                      if block.settings.divider
                        echo 'form-blocks '
                      endif
                      echo 'clearfix"'
                      echo block.shopify_attributes
                      echo '>'
                      render 'product.price', product: product, price_size: block.settings.price_size, price_msg: block.settings.price_msg
                      echo '</div>'
                      
                    when 'payments'
                      render 'product.payments', form: form, block: block, divider: block.settings.divider
                    
                    when 'availability'
                      render 'product.availability', product: product, block: block, variant: variant, product_availability_total: block.settings.product_availability_total, divider: block.settings.divider, banner: false
                    
                    when 'properties'
                      render 'product.properties', block: block, text: block.settings.text, text_label: block.settings.text_label, select_title: block.settings.select_title, select_body: block.settings.select_body, select_label: block.settings.select_label, checkbox: block.settings.checkbox, radio_body: block.settings.radio_body, radio_label: block.settings.radio_label, divider: block.settings.divider
                    
                    when 'variants'
                      assign product_variants = true
                      echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks no-pad '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        render 'product.variants', product: product, variant: variant, variant_enable: block.settings.variant_enable, variant_color_enable: block.settings.variant_color_enable, var_length_enable: block.settings.var_length_enable, p_sect: true, lazy: lazy
                      echo '</div>'
                    
                    when 'quantity'
                      echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks no-pad '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        render 'product.quantity', variant: variant
                      echo '</div>'
                    
                    when 'form'
                      echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks no-pad '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        if block.settings.gift_enable and product.gift_card?
                          render 'product.gift', product: product, block: block, form: form
                        endif
                        render 'product.buttons', form: form, product: product, variant: variant, block: block, smart_enable: block.settings.smart_enable, banner: false
                      echo '</div>'
            
                      when 'shipping'
                        echo '<div class="shipping_msg bottompad-quarter '
                          if block.settings.divider
                            echo 'form-blocks '
                          endif
                          echo 'clearfix"'
                          echo block.shopify_attributes
                          echo '>'
                          render 'shipping', size: 26
                        echo '</div>'
                      
                    when 'store'
                      render 'product.storeAvailability', product: product, block: block
                                            
                    when '@app'
                      echo '<div class="bottompad-quarter clearfix"'
                      echo block.shopify_attributes
                      echo '>'
                      render block
                      echo '</div>'
                      
                    when 'description'                      
                      capture desc
                        echo product.description | split: '<h6>' | first
                      endcapture                      
                      if desc != blank
                        echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        echo desc
                        echo '</div>'
                      endif
                      
                    when 'button'
                      if block.settings.link != blank and block.settings.text != blank
                        echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        echo '<a href="'
                        echo block.settings.link
                        echo '" class="btn'
                        if block.settings.outline_button
                          echo ' outline'
                        endif
                        unless block.settings.divider
                          echo ' bottompad-half'
                        endunless
                        echo '" title="'
                        echo block.settings.text
                        echo '">'
                        echo block.settings.text
                        echo '</a>'
                        echo '</div>'
                      endif
                      
                    when 'meta' 
                      assign meta_size = block.settings.meta | strip_html
                      if block.settings.meta != blank and meta_size.size > 0
                        capture onep
                          if block.settings.style == 'small'
                            echo '<p class="meta'
                            if block.settings.divider
                              echo ' offers no-pad'
                            endif
                            echo '"><small>'
                          else
                            echo block.settings.style | prepend: '<' | append: ' class="meta'
                            if block.settings.divider
                              echo ' offers no-pad'
                            endif
                            echo '">'
                          endif
                        endcapture
                        capture twop
                          if block.settings.style == 'small'
                            echo '</small></p>'
                          else
                            echo block.settings.style | prepend: '</' | append: '>'
                          endif
                        endcapture
                        echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks meta '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        echo block.settings.meta | replace: '<p>', onep | replace: '</p>', twop
                        echo '</div>'
                      endif
                      
                    when 'liquid'
                      assign liquid_size = block.settings.liquid | strip_html
                      if block.settings.liquid != blank and liquid_size.size > 0
                        echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks '
                        else
                          echo 'bottompad-quarter '                            
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        echo block.settings.liquid
                        echo '</div>'
                      endif
                    
                    when 'image'
                      echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks '
                        else
                          echo 'bottompad-quarter '
                        endif
                      echo 'clearfix"'
                      echo block.shopify_attributes
                      echo '><div class="relative lazycontainer table icon-text-table'
                      if section.settings.centered
                        echo ' center'
                      endif
                      echo '">'
                      capture alt
                        if block.settings.block_img.alt != blank
                          echo block.settings.block_img.alt
                        elsif block.settings.caption != blank
                          echo block.settings.caption
                        else
                          echo block.settings.block_img | split: '/' | last | replace: '-', ' ' | replace: '_', ' ' | split: '.' | first | capitalize
                        endif
                      endcapture
                      if block.settings.block_img != blank
                        render 'image.load', img_src: block.settings.block_img, size: 560, alt: alt, lazy: lazy, sizes: '(min-width: 600px) 340px, (min-width: 768px) 560px, calc(100vw - 40px)'
                      else
                        echo 'image' | placeholder_svg_tag: 'placeholder-svg'
                      endif
                      echo '</div></div>'
                    
                    when 'icon'
                      echo '<div class="'
                      if block.settings.divider
                      echo 'form-blocks '
                      else
                      echo 'bottompad-quarter '
                      endif
                      echo 'clearfix"'
                      echo block.shopify_attributes
                      echo '><div class="table icon-text-table'
                      if section.settings.centered
                      echo ' center'
                      endif
                      echo '">'
  
                      for i in (1..3)
  
                        assign enable_number = 'enable_' | append: i                          
                        assign enable = block.settings[enable_number]
  
                        assign icon_number = 'icon_' | append: i                          
                        assign icon = block.settings[icon_number]
  
                        assign block_img_number = 'block_img_' | append: i                          
                        assign block_img = block.settings[block_img_number]
  
                        assign icon_color_number = 'icon_color_' | append: i                          
                        assign icon_color = block.settings[icon_color_number]
  
                        assign icon_color_h_number = 'icon_color_h_' | append: i                          
                        assign icon_color_h = block.settings[icon_color_h_number]
  
                        assign text_number = 'text_' | append: i                          
                        assign text = block.settings[text_number]
  
                        assign pop_up_number = 'pop_up_' | append: i                          
                        assign pop_up = block.settings[pop_up_number]
  
                        assign page_number = 'page_' | append: i                          
                        assign page = block.settings[page_number]
  
                        render 'product.icons', enable_number: enable_number, enable: enable, icon: icon, block_img: block_img, icon_color: icon_color, icon_color_h: icon_color_h, text: text, pop_up: pop_up, page: page
  
                      endfor
  
                      echo '</div></div>'
                  
                    when 'divider'
                      echo '<div class="form-blocks no-pad clearfix"'
                      echo block.shopify_attributes
                      echo '></div>'
                      
                  endcase
                endfor
                
                unless product_variants
                  echo '<div class="hidden">'
                    render 'product.variants', product: product, variant: variant
                  echo '</div>'
                endunless
                
              -%}
  
              {%- render 'product.json', product: product -%}
  
              {% endform %}



            {% else %}
              <div class="product-title clearfix"><h2 class="h1">{{ 'homepage.onboarding.product_title' | t }}</h2></div>                            
              <p class="product-logistics">
                <span class="vendor"><a href="/collections/all" title="{{ shop.name | escape }}">{{ shop.name | escape }}</a></span>
                <span class="separator separator_2" aria-hidden="true"></span>
                <span class="variant-sku"><span>{{ 'products.product.sku' | t }} </span>{{ 'now' | date: '%m%d%y' }}</span>
              </p>
              <h2 class="offers">
                <span class="price-preview price price-field product-single__price">{% if settings.currency_enable %}{{ 1999 | money_with_currency }}{% else %}{{ 1999 | money }}{% endif %} <del>{% if settings.currency_enable %}{{ 2499 | money_with_currency }}{% else %}{{ 2499 | money }}{% endif %}</del></span>
                <span class="savings"><span>{{ 'products.product.savings' | t }}</span> <span>{% if settings.currency_enable %}{{ 500 | money_with_currency }}{% else %}{{ 500 | money }}{% endif %}</span></span>
              </h2>
              <div class="bottompad-quarter clearfix">
                <span class="variant-inventory">
                  <h4>{{ 'products.product.sold_out' | t }}</h4>
                </span>
              </div>
              <div class="clearfix">
                <span class="quantity form">
                  <button type="button" class="btn qtybtn icon-minus ignore text-center" aria-label="{{ 'products.general.minus' | t | escape }}">{%- render 'icons.theme', icon: 'minus', size: 28 -%}</button>
                  <label for="updates_{{ section.id }}" class="visually-hidden">{{ 'products.general.quantity' | t }}</label>
                  <input name="quantity" type="number" inputmode="numeric" pattern="[0-9]*" value="0" min="0" max="0" class="ignore quantity item-qty text-center"/>                   
                  <button type="button" class="btn qtybtn icon-plus ignore text-center" aria-label="{{ 'products.general.plus' | t | escape }}">{%- render 'icons.theme', icon: 'plus', size: 28 -%}</button>       
                  <label class="stock error hidden">.</label>
                </span>
              </div>
              <div class="purchase-section">
                <div class="bottompad-quarter">
                  <input type="submit" class="btn auto-full-width disabled" disabled="disabled" value="{{ 'products.product.unavailable' | t }}" />
                </div>
              </div>
            {% endunless %}
          </div>
        </div>
      </div>
    </div>
  </div>
  
</section>

{% schema %}
  {
    "name": "t:sections.featured_product.name",
    "settings": [
      {
        "type": "header",
        "content": "t:sections.banner.label"
      },
      {
        "type": "image_picker",
        "id": "background_img_d",
        "label": "t:sections.image.desktop.label",
        "info": "t:sections.image.desktop.info"
      },
      {
        "type": "image_picker",
        "id": "background_img_m",
        "label": "t:sections.image.mobile.label",
        "info": "t:sections.image.mobile.info"
      },

	  {
       "type": "checkbox",
       "id": "full_width",
       "label": "t:sections.full_width.label",
       "default": true
      },
      {
        "type": "range",
        "id": "padding_desktop",
        "min": 5,
        "max": 100,
        "step": 5,
        "unit": "%",
        "label": "t:sections.height.desktop.label",
        "default": 25
      },
      {
        "type": "range",
        "id": "padding_mobile",
        "min": 5,
        "max": 100,
        "step": 5,
        "unit": "%",
        "label": "t:sections.height.mobile.label",
        "default": 25
      },
	  {
        "type": "color",
        "id": "background_img_overlay",
        "label": "t:sections.overlay.label"
      },
      {
        "type": "color_background",
        "id": "background_img_overlay_gradient",
        "label": "t:sections.overlay.gradient"
      },
      {
        "type": "range",
        "id": "background_img_overlay_opacity",
        "min": 0,
        "max": 100,
        "step": 5,
        "unit": "%",
        "label": "t:sections.opacity.label",
        "default": 0
      },
      {
        "type": "header",
        "content": "t:sections.media.label"
      },
	  {
        "type": "paragraph",
        "content": "t:sections.media.info"
      },
      {
        "type": "checkbox",
        "id": "img_align",
        "label": "t:settings_schema.products.settings.options_ratio.label",
        "info": "t:settings_schema.products.settings.options_ratio.info_2",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "zoom_enable",
        "label": "t:sections.enable_zoom.label",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "zoom_mobile_enable",
        "label": "t:sections.enable_mobile_zoom.label",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "loop_enable",
        "label": "t:sections.enable_video_looping.label",
        "default": true
      },
      {
        "type": "header",
        "content": "t:sections.options.label"
      },
      {
        "id": "product",
        "type": "product",
        "label": "t:sections.product.name"
      },
      {
        "type": "checkbox",
        "id": "two_col_enable",
        "label": "t:sections.two_column.label",
        "default": true
      },
      {
        "type": "select",
        "id": "vertical_content_alignment",
        "label": "t:sections.vertical_position.label",
        "default": "middle",
        "options": [
          {
            "value": "top",
            "label": "t:sections.vertical_position.top"
          },
          {
            "value": "middle",
            "label": "t:sections.vertical_position.middle"
